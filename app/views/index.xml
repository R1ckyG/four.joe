<Alloy>
  <TabGroup backgroundColor="white">
    <Tab id="profile" title="My Profile">
     <Window id="profileWin" title="My Profile">
      <Require src="studentinfo"/>
      
      <TableView filterAttribute="title" id="course_list">
        <TableViewSection headerTitle="My Courses">
          <TableViewRow class="course_row">
            <Label class="course_name" text="foo"/>
            <Label class="messages" text="(4)"/>
          </TableViewRow>
          <TableViewRow class="course_row">
            <Label class="course_name" text="bar"/>
            <Label class="messages" text="(7)"/>
          </TableViewRow>
          <TableViewRow class="course_row">
            <Label class="course_name" text="boo"/>
            <Label class="messages" text="(3)"/>
          </TableViewRow>
        </TableViewSection>
      </TableView>
      <TableView id="menu_items">
        <TableViewRow id="settings" class="course_row">
          <Label id="settings_label" text='Settings'/>
        </TableViewRow>
        <TableViewRow id="stats" class="course_row">
          <Label id="stats_label" text='My Stats'/>
        </TableViewRow>
      </TableView>
     </Window>
    </Tab>
     <Tab id="course" title="My Classes">
      <Window id="courseWin" title="My Courses">
       <Label id="label" onClick="doClick">Classes go here</Label>
      </Window>
     </Tab>
   </TabGroup>
</Alloy>
